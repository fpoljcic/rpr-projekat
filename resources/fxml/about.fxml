<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" prefHeight="97.0" prefWidth="158.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.projekat.AboutController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="176.0" minWidth="10.0" prefWidth="40.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="272.0" minWidth="10.0" prefWidth="166.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="17.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="47.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label alignment="CENTER" maxHeight="Infinity" maxWidth="Infinity" text="O meni" GridPane.columnSpan="2">
         <GridPane.margin>
            <Insets top="3.0" />
         </GridPane.margin></Label>
        <Label fx:id="aboutText" alignment="CENTER" maxHeight="Infinity" maxWidth="Infinity" prefWidth="238.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="5.0" />
         </GridPane.margin></Label>
        <Button fx:id="buttonOk" maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" onAction="#buttonOkClick" text="OK" GridPane.columnSpan="2" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" />
         </GridPane.margin></Button>
        <ImageView fx:id="imageView" fitHeight="39.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1" />
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</GridPane>
